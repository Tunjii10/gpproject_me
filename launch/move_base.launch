<launch>
  <!-- Run the map server -->
  <arg name="map_file" default="$(find myproject)/map.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  <!-- - Run AMCL -->
  <include file="$(find myproject)/launch/amcl.launch" />
  <!-- <arg name="base_global_planner" default="carrot_planner/CarrotPlanner"/> -->
  
  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find myproject)/params/move_base_params.yaml" command="load" />
    <!-- observation sources located in costmap_common.yaml -->
    <rosparam file="$(find myproject)/costmaps/costmap_common.yaml" command="load" ns="local_costmap"/>
    <rosparam file="$(find myproject)/costmaps/costmap_common.yaml" command="load" ns="global_costmap"/>
     <!-- local costmap, needs size -->
    <rosparam file="$(find myproject)/costmaps/costmap_local.yaml" command="load" ns="local_costmap" />
    <param name="local_costmap/width" value="10.0"/>
    <param name="local_costmap/height" value="10.0"/>
     <!-- global costmap with laser, for odom_navigation_demo -->
    <rosparam file="$(find myproject)/costmaps/costmap_global.yaml" command="load" ns="global_costmap"/>
    <param name="global_costmap/width" value="100.0" />
    <param name="global_costmap/height" value="100.0"/>

  </node>
   
</launch>